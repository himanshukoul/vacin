    name: "zerowth"
    BoxLayout:
        orientation: "vertical"
        size: root.width, root.height
        Label:
            text: "enter MYSQL password"
            font_size: 32
        TextInput:
            multiline: False
            id: mysq
        Button:
            text: "submit"
            font_size: 32
            on_release:
                phoneix = zerowth.backe(mysq.text)
                if phoneix == 1: app.root.current = "eleventh"
                elif phoneix == -1: app.root.current = "twelfth"
                mysq.text = ""
                root.manager.transition.direction = "left"
        Button:
            text: "BACK"
            font_size: 32
            on_release:
                app.root.current = "first"
                root.manager.transition.direction = "left"

<FirstWindow>:
    id: first
    name: "first"
    BoxLayout:
        orientation: "vertical"
        size: root.width, root.height
        Label:
            text: """1 for data input
                2 for accessing data (GOV. officials)
                3 to exit
                4 create Backend
                5 for master changes"""
            font_size: 16
        TextInput:
            multiline: False
            id: fwi
        Button:
            text: "NEXT SCREEN"
            font_size: 32
            on_release:
                snake = first.strt()
                if snake == 0 and fwi.text in ("1","2","3","6"): app.root.current = "thirteenth"
                elif fwi.text == "1": app.root.current = "second"
                elif fwi.text == "2": app.root.current = "third"
                elif fwi.text == "3": app.root.current = "fifth"
                elif fwi.text == "4": app.root.current = "zerowth"
                elif fwi.text == "5": app.root.current = "master"
                root.manager.transition.direction = "left"
                fwi.text = ""

